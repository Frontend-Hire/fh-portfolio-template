{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@layouts/*": ["src/layouts/*"],
      "@utils/*": ["src/utils/*"],
      "@lib/*": ["src/lib/*"],
      "@constants/*": ["src/constants/*"],
      "@types": ["src/types"],
      "@data/*": ["src/data/*"],
      "@src/*": ["src/*"]
    }
  }
}
